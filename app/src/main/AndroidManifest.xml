<manifest package="com.brayanbedritchuk.zerotohero"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".view.main.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

        <activity android:name=".view.workout_list.WorkoutListActivity"/>

        <activity android:name=".view.insert_or_edit_workout.InsertOrEditWorkoutActivity"/>

        <activity android:name=".view.workout_details.WorkoutDetailsActivity"/>

        <activity android:name=".view.exercise_chooser.ExerciseChooserActivity"/>

    </application>

</manifest>
