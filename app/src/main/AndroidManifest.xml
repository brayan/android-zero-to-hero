<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.sailboat.zerotohero">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".view.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.workout.insert_or_edit.InsertOrEditWorkoutActivity" />

        <activity android:name=".view.workout.details.WorkoutDetailsActivity" />

        <activity android:name=".view.exercise.chooser.ExerciseChooserActivity" />

        <activity android:name=".view.exercise.insert_or_edit.InsertOrEditExerciseActivity" />

        <activity android:name=".view.exercise.details.ExerciseDetailsActivity" />

    </application>

</manifest>
